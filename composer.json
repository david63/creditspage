{
	"name": "david63/creditspage",
	"type": "phpbb-extension",
	"description": "Adds a link to a credits page showing extension credits.",
	"version": "2.1.0-RC3",
	"time": "2019-06-08",
	"keywords": [
		"phpbb",
		"extension",
		"cookie",
		"privacy",
		"ACP",
		"EU",
		"GDPR"
	],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "David Wood",
			"role": "Extension Developer"
		}
    ],
	"require": {
        "php": ">=5.4.0",
        "composer/installers": "~1.0"
    },
	"extra": {
		"display-name": "[+] Credits page",
		"soft-require": {
			"phpbb/phpbb": ">=3.2.7"
		},
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/3.2",
        	"filename": "/creditspage_version_file.json"
    	}
	}
