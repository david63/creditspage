{
	"name": "david63/creditspage",
	"type": "phpbb-extension",
	"description": "Adds a link to a credits page showing extension credits.",
	"version": "3.1.0-RC1",
	"time": "2019-06-08",
	"keywords": [
		"phpbb",
		"extension",
		"cookie",
		"privacy",
		"ACP",
		"EU",
		"GDPR"
	],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "David Wood",
			"role": "Extension Developer"
		}
    ],
	"require": {
        "php": ">=7.1.3",
        "composer/installers": "~1.0"
    },
	"extra": {
		"display-name": "[*] Credits page",
		"soft-require": {
			"phpbb/phpbb": ">=3.3.0"
		},
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/versions",
        	"filename": "creditspage_version_file.json",
			"ssl": false
    	}
	}
}
